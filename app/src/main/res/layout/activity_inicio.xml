<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimaryDark"
    android:visibility="visible"
    tools:context=".view.Inicio">

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="10dp"
        android:background="@color/background"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.473">

    </LinearLayout>

    <ImageView
        android:id="@+id/btnInicioGerarJogoManualmente"
        android:layout_width="150dp"
        android:layout_height="63dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:layout_weight="1"
        app:layout_constraintBottom_toTopOf="@+id/btnInicioRelatorio"
        app:layout_constraintEnd_toEndOf="@+id/btnInicioRelatorio"
        app:layout_constraintHorizontal_bias="0.407"
        app:layout_constraintStart_toStartOf="@+id/btnInicioRelatorio"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/btn_gerar_branco" />

    <TextView
        android:id="@+id/txtInicioGerarjogoManualmente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_weight="1"
        android:text="Manualmente"
        android:textColor="@color/texto_cor"
        android:textSize="@dimen/texto_normal"
        app:layout_constraintBottom_toTopOf="@+id/btnInicioGerarJogoManualmente"
        app:layout_constraintEnd_toEndOf="@+id/btnInicioGerarJogoManualmente"
        app:layout_constraintHorizontal_bias="0.593"
        app:layout_constraintStart_toStartOf="@+id/btnInicioGerarJogoManualmente" />

    <ImageView
        android:id="@+id/btnInicioVerificarApostas"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/btnInicioRelatorio"
        app:layout_constraintEnd_toEndOf="@+id/btnInicioGerarJogo"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnInicioGerarJogo"
        app:layout_constraintTop_toTopOf="@+id/btnInicioRelatorio"
        app:srcCompat="@drawable/btn_verificar_branco" />

    <ImageView
        android:id="@+id/btnInicioInformacoes"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/linearLayout3"
        app:layout_constraintEnd_toEndOf="@+id/btnInicioRelatorio"
        app:layout_constraintStart_toStartOf="@+id/btnInicioRelatorio"
        app:layout_constraintTop_toBottomOf="@+id/btnInicioRelatorio"
        app:srcCompat="@drawable/btn_informacao_branco" />

    <ImageView
        android:id="@+id/btnInicioGerarJogo"
        android:layout_width="150dp"
        android:layout_height="63dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/btnInicioRelatorio"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.407"
        app:layout_constraintStart_toEndOf="@+id/btnInicioRelatorio"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/btn_gerar_branco" />

    <ImageView
        android:id="@+id/btnInicioRelatorio"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/linearLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.134"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout3"
        app:srcCompat="@drawable/btn_estatistica_branco" />

    <TextView
        android:id="@+id/txtInicioInformacoes"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="Informações sobre a Mega Sena"
        android:textAlignment="center"
        android:textColor="@color/texto_cor"
        android:textSize="@dimen/texto_normal"
        app:layout_constraintBottom_toTopOf="@+id/btnInicioInformacoes"
        app:layout_constraintEnd_toEndOf="@+id/btnInicioInformacoes"
        app:layout_constraintStart_toStartOf="@+id/btnInicioInformacoes" />

    <TextView
        android:id="@+id/txtInicioRelatorio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="Relatorio de Apostas"
        android:textColor="@color/texto_cor"
        android:textSize="@dimen/texto_normal"
        app:layout_constraintBottom_toTopOf="@+id/btnInicioRelatorio"
        app:layout_constraintEnd_toEndOf="@+id/btnInicioRelatorio"
        app:layout_constraintStart_toStartOf="@+id/btnInicioRelatorio" />

    <TextView
        android:id="@+id/txtInicioVerificarApostas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="Verificar Apostas"
        android:textColor="@color/texto_cor"
        android:textSize="@dimen/texto_normal"
        app:layout_constraintBottom_toTopOf="@+id/btnInicioVerificarApostas"
        app:layout_constraintEnd_toEndOf="@+id/btnInicioVerificarApostas"
        app:layout_constraintStart_toStartOf="@+id/btnInicioVerificarApostas" />

    <TextView
        android:id="@+id/txtInicioGerarjogo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Aleatório"
        android:textColor="@color/texto_cor"
        android:textSize="@dimen/texto_normal"
        app:layout_constraintBottom_toTopOf="@+id/btnInicioGerarJogo"
        app:layout_constraintEnd_toEndOf="@+id/btnInicioGerarJogo"
        app:layout_constraintStart_toStartOf="@+id/btnInicioGerarJogo" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="Mega Sorteio"
        android:textColor="@color/texto_cor"
        android:textSize="@dimen/texto_grande"
        app:layout_constraintBottom_toTopOf="@+id/txtInicioGerarjogo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.13"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.45999998" />
</android.support.constraint.ConstraintLayout>